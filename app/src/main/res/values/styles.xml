<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/MarromColor1</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/MarromColor3</item>
        <item name="android:colorForeground">@color/WhiteColor</item>
        <item name="android:navigationBarColor">@color/BlackColor</item>
        <item name="android:textColorPrimary">@color/BackgroundColor</item>
        <item name="android:colorBackground">@color/BackgroundColor</item>
        <item name="android:textColorSecondary">@color/BlackColor</item>
        <item name="android:textColorSecondaryInverse">@color/WhiteColor</item>

    </style>



    <style name = "AppTheme.Custom">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
</resources>
